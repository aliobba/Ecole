<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.EspaceAdmin.EmploiAdmin"
    android:orientation="vertical"
    >


    <com.github.eunsiljo.timetablelib.view.TimeTableView
        android:id="@+id/timeTableAdmin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <TextView
        android:id="@+id/addEmploiTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Veuillez ajouter un emploi"
        android:layout_centerInParent="true"
        android:textSize="50dp"
        />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/addEmploi"
        android:layout_width="500dp"
        android:layout_height="300dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="26dp"
        android:backgroundTint="#e8b201"
        android:src="@drawable/addbtn"
        android:visibility="invisible"
        />
</RelativeLayout>